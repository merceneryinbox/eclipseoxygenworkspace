package epamLecturesTest;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.IOException;
import java.io.InputStream;

public class MainLectureTestEpam {
    static String fileName = "src\\epamLecturesTeast\\MainLectureTestEpam.java";

    public MainLectureTestEpam() {

    }

    public static void main(String[] args) {

	try (InputStream iStream = new FileInputStream(fileName)) {
	    System.out.println(iStream.toString());
	} catch (FileNotFoundException e) {
	    e.printStackTrace();
	} catch (IOException e1) {
	    e1.printStackTrace();
	}
    }

}
